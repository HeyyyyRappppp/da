while wait() do
pcall(function()
for i,v in pairs(game:GetService("Players"):GetPlayers()) do
if i == 20 then
plyserv:Set("Players :".." "..i.." ".."/".." ".."20".." ".."(Max)")
elseif i == 1 then
plyserv:Set("Player :".." "..i.." ".."/".." ".."20")
else
plyserv:Set("Players :".." "..i.." ".."/".." ".."20")
end
end
end)
end

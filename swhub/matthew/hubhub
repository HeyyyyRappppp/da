game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[_G.SelectPly].Character.HumanoidRootPart.CFrame
wait(.5)
local A_1 = "HE"
local A_2 = game:GetService("Workspace")[_G.SelectPly].SWORD.HitPart

VM:AddSeperator("Visual Total Power")
VM:AddButton('999.99Ggp',function()
local Character = game.Players.LocalPlayer.Character
for _, v in pairs(Character:GetChildren()) do
if v.Name == "HumanoidRootPart"
then for _, v2 in pairs(v:GetChildren()) do
if v2.Name == "TotalPower"
then for _, v3 in pairs(v2:GetChildren()) do
if v3.Name == "Num" then
v3.Text = "999.99Ggp"
end
end
end
end
end
end
end)

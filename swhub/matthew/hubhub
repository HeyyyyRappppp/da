CRs:AddSeperator("Highlights Mode")
CRs:AddToggle("Highlight Mode",false,function(value)
        if value == true then
            local Notif = require(game.Players.LocalPlayer.PlayerGui.Main.MainClient.NotifModule)
            Notif.Notify("Highlight Mode On")
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Category.List.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Category.Buttons.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChikaraPopUp.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Cooldown.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.CurrencyFrame.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Dialogue.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Hotkeys.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.MobileFlight.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.MobileKeys.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.YenPopUp.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Timer.Visible = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Chat.Enabled = false
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.TradePopUp:Destroy()
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.MultEvent:Destroy()
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.SafeZone:Destroy()
        else
        local Notif = require(game.Players.LocalPlayer.PlayerGui.Main.MainClient.NotifModule)
        Notif.Notify("Highlight Mode Off")
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Category.List.Visible = true
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Category.Buttons.Visible = true
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChikaraPopUp.Visible = true
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Cooldown.Visible = true
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.CurrencyFrame.Visible = true
            game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Dialogue.Visible = true
